{
	"name": "super",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "drizzle-kit push && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ci": "vitest --run",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"add-migration": "pnpm drizzle-kit generate",
		"inspect": "pnpm drizzle-kit studio",
		"seed": "drizzle-kit push && vite-node --options.transformMode.ssr='/.*/'  src/lib/seeders/seed.ts"
	},
