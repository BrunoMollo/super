		"globals": "^15.0.0",
		"lucide-svelte": "^0.424.0",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.4",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-headless-table": "^0.18.2",
