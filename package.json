		"@tailwindcss/typography": "^0.5.13",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/bcrypt": "^5.0.2",
		"@types/eslint": "^9.6.0",
		"@types/pg": "^8.11.6",
		"@vitest/coverage-istanbul": "^2.0.5",
		"autoprefixer": "^10.4.19",
